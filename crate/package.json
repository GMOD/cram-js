{
  "name": "inflate-wasm-build",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:wasm": "cargo build --release --target wasm32-unknown-unknown && ~/.cargo/bin/wasm-bindgen --target bundler --out-dir ../src/wasm target/wasm32-unknown-unknown/release/inflate_wasm.wasm",
    "build:bundle": "webpack",
    "build": "yarn build:wasm && yarn build:bundle"
  },
  "devDependencies": {
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  }
}
